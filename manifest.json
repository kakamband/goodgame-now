{
    "name": "GoodGame Reminder",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [ "*://*.goodgame.ru/*" ]
        }
    ],
    "browser_action": {
        "default_icons": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "64": "/icons/64.png"
        },
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "notifications"
    ]
}